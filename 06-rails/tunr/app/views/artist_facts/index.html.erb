<h1>Listing artist_facts</h1>

<table>
  <tr>
    <th>Fact</th>
    <th>Website</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @artist_facts.each do |artist_fact| %>
  <tr>
    <td><%= artist_fact.fact %></td>
    <td><%= artist_fact.website %></td>
    <td><%= link_to 'Show', artist_fact %></td>
    <td><%= link_to 'Edit', edit_artist_fact_path(artist_fact) %></td>
    <td><%= link_to 'Destroy', artist_fact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Artist fact', new_artist_fact_path %>
<%= link_to 'Edit all artist facts', edit_all_artist_facts_path %>
